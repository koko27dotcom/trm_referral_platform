{
  "name": "myan-jobs",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "start": "node server/server.js",
    "server": "node server/server.js",
    "server:dev": "nodemon server/server.js",
    "seed": "node server/seeders/index.js",
    "seed:users": "node server/seeders/index.js --users",
    "seed:jobs": "node server/seeders/index.js --jobs",
    "seed:market": "node server/seeders/index.js --market",
    "seed:academy": "node server/seeders/index.js --academy",
    "seed:clear": "node server/seeders/index.js --clear",
    "payment:reconcile": "node server/scripts/reconcilePayments.js",
    "payment:stats": "node server/scripts/paymentStats.js",
    "security:audit": "node server/scripts/securityAudit.js",
    "security:rotate-keys": "node server/scripts/rotateEncryptionKeys.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration",
    "test:ci": "jest --ci --coverage --reporters=default --reporters=jest-junit",
    "test:unit:frontend": "vitest run",
    "test:unit:frontend:watch": "vitest",
    "test:unit:frontend:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:unit:frontend && npm run test:e2e",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "typecheck": "tsc --noEmit",
    "perf:cache:clear": "node -e \"require('./server/services/enhancedCacheService.js').clear()\"",
    "perf:cache:stats": "node -e \"console.log(require('./server/services/enhancedCacheService.js').getStats())\"",
    "perf:cache:warm": "node -e \"require('./server/services/cacheWarmingService.js').warmAll()\"",
    "db:indexes:create": "node -e \"require('./server/services/databaseIndexService.js').createAllIndexes().then(console.log)\"",
    "db:indexes:health": "node -e \"require('./server/services/databaseIndexService.js').getHealthReport().then(console.log)\"",
    "db:indexes:verify": "node -e \"require('./server/services/databaseIndexService.js').verifyIndexes().then(console.log)\"",
    "monitor:queries:slow": "node -e \"console.log(require('./server/services/queryOptimizationService.js').getSlowQueries(10))\"",
    "monitor:queries:stats": "node -e \"console.log(require('./server/services/queryOptimizationService.js').getStats())\""
  },
  "dependencies": {
    "@sendgrid/mail": "^8.1.6",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.23",
    "bcryptjs": "^3.0.3",
    "bull": "^4.16.5",
    "canvas-confetti": "^1.9.4",
    "chart.js": "^4.5.1",
    "cors": "^2.8.6",
    "dotenv": "^17.2.3",
    "ethers": "^6.16.0",
    "express": "^5.2.1",
    "framer-motion": "^11.0.0",
    "i18next": "^25.8.0",
    "i18next-http-backend": "^3.0.2",
    "ioredis": "^5.9.2",
    "jsonwebtoken": "^9.0.3",
    "lucide-react": "^0.300.0",
    "mongoose": "^9.1.5",
    "multer": "^2.0.2",
    "node-cron": "^4.2.1",
    "openai": "^4.104.0",
    "pdf-parse": "^2.4.5",
    "postcss": "^8.5.6",
    "puppeteer": "^24.36.1",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-adblocker": "^2.13.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "qrcode": "^1.5.3",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.3.1",
    "react-countup": "^6.5.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^16.5.4",
    "react-router-dom": "^6.21.0",
    "recharts": "^3.7.0",
    "stripe": "^14.25.0",
    "tailwindcss": "^3.4.19",
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@playwright/test": "^1.41.0",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/canvas-confetti": "^1.9.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.11.16",
    "@types/node-cron": "^3.0.11",
    "@types/qrcode": "^1.5.5",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.3.7",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitest/coverage-v8": "^1.2.2",
    "babel-jest": "^29.7.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "jest-junit": "^16.0.0",
    "jest-watch-typeahead": "^2.2.2",
    "jsdom": "^24.0.0",
    "mongodb-memory-server": "^9.1.6",
    "msw": "^2.1.5",
    "nodemon": "^3.0.3",
    "prettier": "^3.2.5",
    "supertest": "^6.3.4",
    "vitest": "^1.2.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
